parameter sigmap(r,i,v1) CES substitution elasticity between ND and KLEF ;
sigmap(r,i,v1) = 0.0 ;

parameter sigmav(r,i,v1) CES substitution elasticity between KE and L ;
sigmav(r,i,'Old') = 0.12 ;
sigmav(r,i,'New') = 1.01 ;

parameter sigmal(r,i) CES substitution elasticity across different labour skills ;
sigmal(r,i) = 0.50 ;

parameter sigmae(r,i,v1) CES substitution elasticity between K and E ;
sigmae(r,i,'Old') = 0.00 ;
sigmae(r,i,'New') = 0.80 ;

parameter sigmak(r,i,v1) CES substitution elasticity between K and fixed factor ;
sigmak(r,i,v1)    = 0.0 ;
sigmak(r,i,'New') = 0.0 ;

parameter sigmaEp(r,i,v1) CES substitution elasticity across fuels ;
sigmaEp(r,i,'Old') = 0.25 ;
sigmaEp(r,i,'New') = 2.00 ;

parameter sigmaf(r,i,v1) CES between fert and KT or feed and land ;
sigmaf(r,cr,v1) = 0.50 ;
sigmaf(r,lv,v1) = 0.50 ;

parameter sigmaa(r,i) CES across fert in crops or across feed in livestock ;
sigmaa(r,cr) = 0.50 ;
sigmaa(r,lv) = 0.50 ;

parameter sigmac(r,k,h) Transition matrix substitution elasticities ;
sigmac(r,k,h) = 0 ;

parameter sigmam(r,i) Economywide Armington elasticity ;
sigmam(r,'agrfd       ') =     2.3;
sigmam(r,'enrgy       ') =     2.6;
sigmam(r,'manuf       ') =     3.0;
sigmam(r,'srvcs       ') =     1.9;

parameter sigmamp(r,i,j)    Armington elasticities in production ;
parameter sigmamc(r,i,h)    Armington elasticities in household consumption ;
parameter sigmamg(r,i)      Armington elasticities in government consumption ;
parameter sigmami(r,i)      Armington elasticities in investment ;
parameter sigmaw(r,r2,i)    Fourth level Armington elasticity ;
parameter sigmaw1(r,i)      Second level Armington elasticity ;
parameter sigmaw2(r,r1,i)   Second level Armington elasticity ;
parameter sigmax(r,i)       Top level CET elasticity between domestic and export sales ;
parameter sigmaz(r,i)       Second level CET elasticity for sales across export markets ;

sigmamp(r,i,j)  = sigmam(r,i) ;
sigmamc(r,i,h)  = sigmam(r,i) ;
sigmamg(r,i)    = sigmam(r,i) ;
sigmami(r,i)    = sigmam(r,i) ;
sigmaw1(r,i)    = 2*sigmam(r,i) ;
sigmaw(r,r2,i)  = sigmaw1(r,i) ;
sigmaw2(r,r1,i) = sigmaw1(r,i) ;
sigmax(r,i)     = sigmaw1(r,i) ;
sigmaz(r,i)     = sigmax(r,i) ;
sigmax(r,i)     = inf ;
sigmaz(r,i)     = inf ;

parameter yelasArmD(r,i) Initial domestic income elasticities for the Armington function ;
parameter yelasArmM(r,i) Initial import income elasticities for the Armington function ;
yelasArmD(r,i) = 1 ;
yelasArmM(r,i) = 1 ;

parameter sigmab(r,i) CES substitution elasticity between domestic goods and intraregional imports ;
sigmab(r,i) = sigmam(r,i) ;

parameter yelasAIDS0(rp,r,i) Initial income elasticities for the AIDS function ;
yelasAIDS0(rp,r,i) = 1 ;

parameter sigmaq(rp,rpp,r,i) AIDS substitution elasticities ;
sigmaq(rp,rpp,r,i)$(ord(rp) ne ord(rpp)) = sigmam(r,i) ;

parameter omegatl(r) Land CET transformation elasticity across agricultural sectors ;
omegatl(r) = inf ;

parameter leps0(r) Initial aggregate land supply elasticity /
   jpn           0.25
   usa           1.00
   eur           0.25
   rhy           1.00
   liy           1.00
   miy           1.00
/ ;

*  LandMax represents the maximum amount of land. To use the normal
*  supply function, set LandMax to infinity. To use the logistical
*  supply function, LandMax should be entered as some ratio compared
*  to the initial land value. For example, a value of 1.1 indicates
*  that land can expand to about 10% above its initial level.

parameter LandMax(r) Maximum amount of land ;
LandMax(r) = inf ;

parameter omegam(r,ll) Migration elasticity ;
omegam(r,ll) = inf ;

parameter labd0(r,ll,i) Labor volume by skill and sector ;
labd0(r,ll,i) = 0 ;
labd0(r,ll,i) = bscale*labd0(r,ll,i) ;

parameter migr0(r,ll) Rural to urban migration ;
migr0(r,ll) = 0 ;

parameter ue0(r,ll,gz) Unemployment rates ;
ue0(r,ll,gz) = 0 ;

*  ueFlag, set to 1 to if UE is fixed and minimum wage function is calibrated
*                 0       with UE edogenous

parameter ueFlag(r,ll) ;
ueFlag(r,ll) = 0 ;

* The minimum wage is linked to the unemployment rate
*  If ue is positive, the minimum wage should be set to 1
*  For full employment, set ue to 0 and wmin to 0
*  For initial full employment but with a minimum wage
*  set ue to 0 and wmin to a positive number less than 1
*  This will put a floor on wages
*
*  Note that these rules depend on market segmentation

parameter wmin0(r,ll,gz) Minimum wage ;
wmin0(r,ll,gz) = 0 ;

parameter omegaps(r,ll,gz) Elasticity of the minimum wage wrt to the price of value added ;
omegaps(r,ll,gz) = 0 ;

parameter omegap(r,ll,gz)  Elasticity of the minimum wage wrt to the price index ;
omegap(r,ll,gz) = 1 ;

parameter omegaue(r,ll,gz) Elasticity of the minimum wage wrt to the rate of unemployment ;
omegaue(r,ll,gz) = 1 ;

parameter omegak(r) Capital supply transformation elasticity for comp static simulations ;
omegak(r) = inf ;

