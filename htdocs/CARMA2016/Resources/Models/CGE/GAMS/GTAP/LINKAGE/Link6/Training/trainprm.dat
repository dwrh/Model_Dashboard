omegaf('eur         ', 'manuf       ') =    1.00 ;
omegaf('eur         ', 'srvcs       ') =    1.00 ;
omegaf('rhy         ', 'agrfd       ') =    1.00 ;
omegaf('rhy         ', 'enrgy       ') =    1.00 ;
